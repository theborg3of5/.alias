if [ "$TERM" = "xterm" ]
then

	# Specific to this particular machine. Not to be synced.
	. ~/.alias/specific.alias


	# Alias meta-commands.
	. ~/.alias/alias.alias

	# Variations on common bash commands.
	. ~/.alias/bash.alias

	# AHK Stuff.
	. ~/.alias/ahk.alias

	# Git Stuff.
	. ~/.alias/git.alias

	# SVN Stuff.
	. ~/.alias/svn.alias

	# Fun Stuff.
	. ~/.alias/fun.alias


	# WFU Stuff.
	# . ~/.alias/wfu.alias

	# Shodor-Specific Stuff.
	# . ~/.alias/shodor.alias
fi
